package day8;
import java.io.*;

public class FileDemo2 {

	public static void main(String[] args) throws IOException {
		//  creating a demo.txt file in the directory using createnewfile method
		File f = new File("demo.txt");
		f.createNewFile();
		
		// create a directory named with SaiCharan123 in current working directory and create a file named with abc.txt in that directory
		File f1 = new File("Yeswanth123");
		f1.mkdir();
		File f2 = new File("Yeswanth123", "abc123.txt");
		f2.createNewFile();

	}

}
